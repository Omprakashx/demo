<body>
    <h1>Quora Posts</h1>
    <%for(post of posts) { %>
        <div class="post">
            <h3 class="username">@<%=post.username%></h3>
            <h2 class="content"><%=post.contents%></h2>
            <br/>
            <a href="http://localhost:3000/posts/<%=post.id%>" >see Details</a>
        </div>

   <% } %>
   <a href="http://localhost:3000/posts/new" class="href"> Create new posts</a>
   
</body>
</html>